{
  "name": "nodeproject",
  "version": "1.0.0",
  "main": "server/server.js",
  "bin": {
    "codeclimate": "./bin/codeclimate.js",
    "coveralls": "./bin/coveralls.js"
  },
  "scripts": {
    "start": "node server/server.js",
    "test": "./node_modules/.bin/mocha",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codecov",
    "test-coveralls": "istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test_coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=448f0a49888bf510d14843c1b9760e2591417b17d6d2ffea5ed09604bb9fea0a codeclimate < ./coverage/lcov.info"
  },
  "dependencies": {
    "codecov.io": "^0.1.2",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "coveralls": "^2.11.2",
    "errorhandler": "^1.1.1",
    "loopback": "^2.14.0",
    "loopback-boot": "^2.6.5",
    "loopback-connector-mysql": "^1.7.0",
    "loopback-datasource-juggler": "^2.19.0",
    "mocha-lcov-reporter": "0.0.2",
    "serve-favicon": "^2.0.1"
  },
  "optionalDependencies": {
    "loopback-explorer": "^1.1.0"
  },
  "devDependencies": {
    "jshint": "^2.5.6"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vnysingh/nodeproject.git"
  },
  "description": "nodeproject"
}
