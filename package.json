    "loopback-connector-mysql": "^1.7.0",
    "loopback-datasource-juggler": "^2.19.0",
    "mocha-lcov-reporter": "0.0.2",
    "serve-favicon": "^2.0.1"
  },
  "optionalDependencies": {
    "loopback-explorer": "^1.1.0"
  },
  "devDependencies": {
    "jshint": "^2.5.6"
